package collections;

import java.util.ArrayList;

public class Collections {

	public static void main(String[] args) {
		ArrayList<String> lista = new ArrayList<>();
		lista.add("Juan");
		lista.add("Maria");
		lista.add("Luis");
		
		for (int i = 0; i < lista.size(); i++) {
			if(lista.get(i).startsWith("J")) {
				lista.remove(i);
			}
		}
		
	}
}
